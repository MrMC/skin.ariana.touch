<?xml version="1.0" encoding="UTF-8"?>
<window>
  <defaultcontrol always="true">450</defaultcontrol>
  <controls>
    <include>DialogFullBG</include>
    <control type="image" description="Sidebar">
      <left>-150</left>
      <top>0</top>
      <width>660</width>
      <height>100%</height>
      <aspectratio>stretch</aspectratio>
      <texture border="60">img/HomeMenuBarV.png</texture>
    </control>
    <control type="group">
      <include>OpenClose</include>
      <control type="grouplist" id="9000">
        <left>45</left>
        <top>135</top>
        <width>540</width>
        <height>608</height>
        <itemgap>5</itemgap>
        <onleft>450</onleft>
        <onright>450</onright>
        <onup>9000</onup>
        <ondown>9000</ondown>
        <orientation>vertical</orientation>
        <control type="button" id="413" description="Ok">
          <label>186</label>
          
        </control>
        <control type="button" id="414" description="Cancel">
          <label>222</label>
          
        </control>
        <control type="button" id="415" description="Create folder">
          <label>119</label>
          
        </control>
      </control>
      <control type="label" id="411" description="Heading">
        <include>HeadingR</include>
        <label>-</label>
      </control>
      <control type="panel" id="450">
        <left>1058</left>
        <top>180</top>
        <width>784</width>
        <height>799</height>
        <pagecontrol>60</pagecontrol>
        <onleft>9000</onleft>
        <onright>60</onright>
        <onup>450</onup>
        <ondown>450</ondown>
        <itemlayout width="784" height="80">
          <control type="label" description="Main Label">
            <left>20</left>
            <right>125</right>
            <top>0</top>
            <height>80</height>
            <font>size20</font>
            <label>$INFO[ListItem.Label]</label>
          </control>
          <control type="image" description="arrow">
            <visible>ListItem.IsFolder + IsEmpty(ListItem.label2)</visible>
            <right>20</right>
            <top>0</top>
            <height>80</height>
            <width>40</width>
            <align>right</align>
            <aspectratio>keep</aspectratio>
            <texture colordiffuse="$VAR[LabelNFColor]">img/Arrow.png</texture>
          </control>
          <control type="label" description="Label2">
            <right>20</right>
            <top>0</top>
            <height>80</height>
            <width>124</width>
            <font>size16</font>
            <scroll>false</scroll>
            <align>right</align>
            <label>$INFO[ListItem.Label2]</label>
          </control>
        </itemlayout>
        <focusedlayout width="784" height="80">
          <control type="group">
            
            <control type="image">
              <animation effect="fade" time="100">VisibleChange</animation>
              <left>10</left>
              <top>10</top>
              <width>776</width>
              <height>60</height>
              <texture border="10">focus/FrameFO.png</texture>
              <bordersize>-10</bordersize>
              <aspectratio>stretch</aspectratio>
            </control>
            <control type="label" description="Main Label">
              <left>20</left>
              <right>125</right>
              <top>0</top>
              <height>80</height>
              <font>size20</font>
              <textcolor>$VAR[LabelFOColor]</textcolor>
              <label>$INFO[ListItem.Label]</label>
            </control>
            <control type="image" description="arrow">
              <visible>ListItem.IsFolder + IsEmpty(ListItem.label2)</visible>
              <right>20</right>
              <top>0</top>
              <height>80</height>
              <width>40</width>
              <aspectratio>keep</aspectratio>
              <align>right</align>
              <texture colordiffuse="$VAR[LabelFOColor]">img/Arrow.png</texture>
            </control>
            <control type="label" description="Label2">
              <right>20</right>
              <top>0</top>
              <height>80</height>
              <width>124</width>
              <font>size16</font>
              <align>right</align>
              <textcolor>$VAR[LabelFOColor]</textcolor>
              <label>$INFO[ListItem.Label2]</label>
            </control>
          </control>
        </focusedlayout>
      </control>
      <control type="image" description="Item Thumb NOT FLIPPED">
        <left>600</left>
        <top>480</top>
        <width>350</width>
        <height>350</height>
        <fadetime>200</fadetime>
        <texture>$INFO[ListItem.Thumb]</texture>
        <aspectratio>keep</aspectratio>
      </control>
    </control>
  </controls>
</window>
