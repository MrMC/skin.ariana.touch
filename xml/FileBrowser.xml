<?xml version="1.0" encoding="UTF-8"?>
<window>
  <defaultcontrol always="true">450</defaultcontrol>
  <controls>
    <include>DialogFullBG</include>
    <control type="group">
      <include>OpenClose</include>
      <control type="label" id="411" description="Heading">
        <include>CenterHeading</include>
        <label>-</label>
      </control>
      <control type="panel" id="450">
        <right>0</right>
        <top>96</top>
        <width>50%</width>
        <bottom>82</bottom>
        <viewtype label="535">List</viewtype>
        <preloaditems>2</preloaditems>
        <itemlayout height="122">
          <control type="image">
            <left>5</left>
            <bottom>0</bottom>
            <right>30</right>
            <height>1</height>
            <texture border="1" colordiffuse="$VAR[Label2Color]">dot/Grey.png</texture>
            <aspectratio>stretch</aspectratio>
          </control>
          <control type="image">
            <left>0</left>
            <top>0</top>
            <height>100%</height>
            <width>120</width>
            <texture fallback="DefaultFolder.png" align="left" aligny="center">$VAR[Thumbnail]</texture>
            <bordersize>10</bordersize>
            <aspectratio>scale</aspectratio>
          </control>
          <control type="label" description="Main Label">
            <left>150</left>
            <right>10</right>
            <top>0</top>
            <height>100%</height>
            <font>size22</font>
            <label>$INFO[ListItem.Label]</label>
            <textcolor>$VAR[LabelNFColor]</textcolor>
          </control>
          <control type="label" description="Label2">
            <left>150</left>
            <right>10</right>
            <top>60%</top>
            <height>40%</height>
            <font>size19</font>
            <label>$INFO[ListItem.Label2]</label>
            <textcolor>$VAR[Label2Color]</textcolor>
          </control>
        </itemlayout>
        <focusedlayout height="122">
          <control type="group">
            <control type="image">
              <left>5</left>
              <bottom>0</bottom>
              <right>30</right>
              <height>1</height>
              <texture border="1" colordiffuse="$VAR[Label2Color]">dot/Grey.png</texture>
              <aspectratio>stretch</aspectratio>
            </control>
            <control type="image">
              <left>0</left>
              <top>0</top>
              <height>100%</height>
              <width>120</width>
              <texture fallback="DefaultFolder.png" align="left" aligny="center">$VAR[Thumbnail]</texture>
              <bordersize>10</bordersize>
              <aspectratio>scale</aspectratio>
            </control>
            <control type="label" description="Main Label">
              <left>150</left>
              <right>10</right>
              <top>0</top>
              <height>100%</height>
              <font>size22</font>
              <label>$INFO[ListItem.Label]</label>
              <textcolor>$VAR[LabelNFColor]</textcolor>
            </control>
            <control type="label" description="Label2">
              <left>150</left>
              <right>10</right>
              <top>60%</top>
              <height>40%</height>
              <font>size19</font>
              <label>$INFO[ListItem.Label2]</label>
              <textcolor>$VAR[Label2Color]</textcolor>
            </control>
          </control>
        </focusedlayout>
      </control>
      <control type="image" description="Thumb">
        <left>0</left>
        <top>96</top>
        <width>50%</width>
        <bottom>80</bottom>
        <include>Icon_Fade</include>
        <texture>thumbs/mrmc.png</texture>
        <bordersize>0</bordersize>
        <aspectratio>scale</aspectratio>
      </control>
      <control type="grouplist" id="9000">
        <include>9000GrouplistParameters</include>
        <control type="button" id="413" description="Ok">
          <label>186</label>
          <include>9000Buttons</include>
        </control>
        <control type="button" id="414" description="Cancel">
          <label>222</label>
          <include>9000Buttons</include>
        </control>
        <control type="button" id="415" description="Create folder">
          <label>119</label>
          <include>9000Buttons</include>
        </control>
      </control>
    </control>
  </controls>
</window>
