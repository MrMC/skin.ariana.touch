<?xml version="1.0" encoding="UTF-8"?>
<window>
  <defaultcontrol always="true">50</defaultcontrol>
  <controls>
    <include>FullBackground</include>
    <control type="group">
      <include>OpenClose</include>
      <include condition="[StringCompare(Skin.AspectRatio,16:9) | StringCompare(Skin.AspectRatio,4:3)]">View_50</include>
      <include condition="![StringCompare(Skin.AspectRatio,16:9) | StringCompare(Skin.AspectRatio,4:3)]" content="View_50">
        <param name="Left" value="120"/>
        <param name="Right" value="120"/>
      </include>
      <control type="grouplist" id="9001">
        <include>9000GrouplistParameters</include>
        <control type="button" id="23" description="Play">
          <include>9000Buttons</include>
          <label>208</label>
        </control>
        <control type="button" id="20" description="Shuffle">
          <label>191</label>
          <include>9000Buttons</include>
        </control>
        <control type="button" id="26" description="Repeat">
          <label>-</label>
          <include>9000Buttons</include>
        </control>
        <control type="button" id="21" description="Save">
          <include>9000Buttons</include>
          <label>190</label>
        </control>
      </control>
    </control>
    <include>NowPlaying</include>
  </controls>
</window>
