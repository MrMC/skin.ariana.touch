<?xml version="1.0" encoding="UTF-8"?>
<window>
  <defaultcontrol always="true">20</defaultcontrol>
  <controls>
    <include>FullBackground</include>
    <control type="label" description="Directory Contains">
      <include>CenterHeading</include>
      <label>20344</label>
    </control>
    <control type="group">
      <include>OpenClose</include>
      <centerleft>50%</centerleft>
      <width>1080</width>
      <top>75</top>
      <control type="spincontrolex" id="20" description="Content Type">
        <top>75</top>
        <width>1080</width>
        <onup>29</onup>
        <ondown>21</ondown>
        <onleft>28</onleft>
        <onright>28</onright>
        <textoffsetx>20</textoffsetx>
        <texturenofocus border="9">img/SettingsButtonNF.png</texturenofocus>
        <label>$LOCALIZE[20344]</label>
      </control>
      <control type="panel" id="21" description="Scraper list">
        <top>171</top>
        <width>810</width>
        <height>210</height>
        <onup>20</onup>
        <onleft>5</onleft>
        <onright>20</onright>
        <ondown>5</ondown>
        <itemlayout height="69" width="810">
          <control type="label">
            <left>30</left>
            <width>750</width>
            <height>69</height>
            <font>size20</font>
            <info>ListItem.Label</info>
          </control>
        </itemlayout>
        <focusedlayout height="69" width="810">
          <control type="group">
            <control type="image">
              <animation effect="fade" time="100">VisibleChange</animation>
              <left>8</left>
              <top>8</top>
              <width>776</width>
              <height>53</height>
              <texture border="10">focus/FrameFO.png</texture>
              <bordersize>-10</bordersize>
              <aspectratio>stretch</aspectratio>
            </control>
            <control type="label">
              <left>30</left>
              <width>750</width>
              <height>69</height>
              <font>size20</font>
              <textcolor>$VAR[LabelFOColor]</textcolor>
              <info>ListItem.Label</info>
            </control>
          </control>
        </focusedlayout>
      </control>
      <control type="grouplist" id="5" description="Control Area">
        <top>405</top>
        <width>1080</width>
        <height>450</height>
        <itemgap>3</itemgap>
        <onup>21</onup>
        <ondown>22</ondown>
        <onleft>noop</onleft>
        <onright>noop</onright>
        <orientation>vertical</orientation>
      </control>
      <control type="button" id="7" description="Button Default">
        <width>1080</width>
        <align>left</align>
      </control>
      <control type="radiobutton" id="8" description="RadioButton">
        <width>1080</width>
        <align>left</align>
        <radioleft>1020</radioleft>
      </control>
      <control type="button" id="22" description="Settings">
          <top>795</top>
          <width>1080</width>
          <align>left</align>
          <label>$LOCALIZE[5]</label>
          <onup>5</onup>
          <onleft>28</onleft>
          <onright>29</onright>
          <ondown>28</ondown>
        </control>
      <control type="image" description="Addon Icon">
        <right>0</right>
        <top>189</top>
        <width>180</width>
        <height>180</height>
        <texture fallback="DefaultFolder.png">$INFO[ListItem.Thumb]</texture>
        <aspectratio aligny="top">scale</aspectratio>
      </control>
    </control>
    <control type="grouplist" id="9000">
      <include>9000GrouplistParameters</include>
      <control type="button" id="28" description="OK">
        <include>9000Buttons</include>
        <label>186</label>
        <onleft>noop</onleft>
        <onright>29</onright>
        <onup>22</onup>
        <ondown>20</ondown>
      </control>
      <control type="button" id="29" description="Cancel">
        <include>9000Buttons</include>
        <label>222</label>
        <onleft>28</onleft>
        <onright>noop</onright>
        <onup>22</onup>
        <ondown>20</ondown>
      </control>
    </control>
  </controls>
</window>
