<?xml version="1.0" encoding="UTF-8"?>
<window>
  <defaultcontrol always="true">9</defaultcontrol>
  <controls>
    <include>FullBackground</include>
    <control type="group">
      <include>OpenClose</include>
      <control type="label" id="20" description="Heading">
        <include>CenterHeading</include>
      </control>
      <control type="grouplist" id="9" description="Left Configure Panel">
        <include condition="[StringCompare(Skin.AspectRatio,16:9) | StringCompare(Skin.AspectRatio,4:3)]">LeftMargin</include>
        <include condition="![StringCompare(Skin.AspectRatio,16:9) | StringCompare(Skin.AspectRatio,4:3)]" content="LeftMargin">
          <param name="Left" value="120"/>
        </include>
        <include>VerticalMargins</include>
        <width>25%</width>
        <itemgap>2</itemgap>
        <onleft>30</onleft>
        <onright>2</onright>
        <ondown>9000</ondown>
      </control>
      <control type="button" id="13" description="Default Category Button">
        <align>left</align>
        <width>25%</width>
      </control>
      <control type="grouplist" id="2" description="Right controls">
        <include>VerticalMargins</include>
        <include condition="[StringCompare(Skin.AspectRatio,16:9) | StringCompare(Skin.AspectRatio,4:3)]">RightMargin</include>
        <include condition="![StringCompare(Skin.AspectRatio,16:9) | StringCompare(Skin.AspectRatio,4:3)]" content="RightMargin">
          <param name="Right" value="120"/>
        </include>
        <width>65%</width>
        <itemgap>2</itemgap>
        <ondown>9000</ondown>
        <onleft>9</onleft>
      </control>
      <control type="button" id="3" description="Default Button">
        <width>65%</width>
        <align>left</align>
      </control>
      <control type="radiobutton" id="4" description="Default RadioButton">
        <width>65%</width>
        <align>left</align>
      </control>
      <control type="spincontrolex" id="5" description="Default spincontrolex">
        <width>65%</width>
        <align>left</align>
        <reverse>yes</reverse>
      </control>
      <control type="label" id="7">
        <width>65%</width>
        <align>left</align>
        <label>-</label>
      </control>
      <control type="image" id="6" description="Default Seperator">
        <width>65%</width>
        <height>-3</height>
        <texture>-</texture>
      </control>
      <control type="sliderex" id="8" description="Default Slider Control">
        <width>65%</width>
        <align>left</align>
      </control>
      <control type="grouplist" id="9000" description="Left OK/Cancel Panel">
        <include>9000GrouplistParameters</include>
        <control type="button" id="10" description="OK Button">
          <include>9000Buttons</include>
          <label>186</label>
        </control>
        <control type="button" id="12" description="Load Defaults">
          <include>9000Buttons</include>
          <label>409</label>
        </control>
      </control>
    </control>
  </controls>
</window>
