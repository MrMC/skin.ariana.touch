<?xml version="1.0" encoding="UTF-8"?>
<window>
  <defaultcontrol>-</defaultcontrol>
  <animation effect="fade" time="500">WindowOpen</animation>
  <animation effect="fade" time="200">WindowClose</animation>
  <controls>
    <control type="group" id="0">
      <visible>!Window.IsActive(DialogFullscreenInfo.xml)</visible>
      <include>VisibleFadeEffect</include>
      <left>0</left>
      <top>75</top>
      <animation effect="fade" time="200">VisibleChange</animation>
      <control type="image" description="Info Background">
        <left>0</left>
        <width>100%</width>
        <height>220</height>
        <texture>dot/Black90.png</texture>
      </control>
      <control type="label" id="10" description="row 1 sys label">
        <top>20</top>
        <left>120</left>
        <right>20</right>
        <height>60</height>
        <align>left</align>
        <aligny>center</aligny>
        <font>size17</font>
        <textcolor>white</textcolor>
        <label>-</label>
      </control>
      <control type="label" id="11" description="row 2 sys label">
        <top>80</top>
        <left>120</left>
        <right>20</right>
        <height>60</height>
        <align>left</align>
        <aligny>center</aligny>
        <font>size17</font>
        <textcolor>white</textcolor>
        <label>-</label>
      </control>
      <control type="label" id="12" description="row 3 sys label">
        <top>140</top>
        <left>120</left>
        <right>20</right>
        <height>60</height>
        <align>left</align>
        <aligny>center</aligny>
        <font>size17</font>
        <textcolor>white</textcolor>
        <label>-</label>
      </control>
    </control>
  </controls>
</window>
