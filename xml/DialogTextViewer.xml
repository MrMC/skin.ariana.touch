<?xml version="1.0" encoding="UTF-8"?>
<window>
  <defaultcontrol>61</defaultcontrol>
  <controls>
    <include>DialogFullBG</include>
    <control type="group">
      <include>OpenClose</include>
      <control type="label" id="1" description="Header">
        <top>120</top>
        <centerleft>50%</centerleft>
        <width>100%</width>
        <height>50</height>
        <font>size50B</font>
        <align>center</align>
        <textcolor>$VAR[LabelNFColor]</textcolor>
      </control>
      <control type="textbox" id="5" description="Textarea">
        <top>240</top>
        <centerleft>50%</centerleft>
        <width>70%</width>
        <height>790</height>
        <font>plot</font>
      </control>
      <control type="scrollbar" id="61">
        <right>5%</right>
        <top>250</top>
        <bottom>110</bottom>
        <ondown>61</ondown>
        <onup>61</onup>
      </control>
    </control>
  </controls>
