<?xml version="1.0" encoding="UTF-8"?>
<window>
  <defaultcontrol always="true">10</defaultcontrol>
  <controls>
    <include>FullBackground</include>
    <control type="group">
      <include>OpenClose</include>
      <control type="label" id="1" description="Heading">
        <include>CenterHeading</include>
      </control>
      <control type="textbox" id="9" description="Labels">
        <top>340</top>
        <centerleft>50%</centerleft>
        <width>960</width>
        <height>170</height>
        <align>center</align>
        <font>size22</font>
        <autoscroll time="3000" delay="4000" repeat="5000">true</autoscroll>
      </control>
      <control type="button" id="10" description="No">
        <top>556</top>
        <centerleft>50%</centerleft>
        <width>360</width>
        <label>106</label>
        <onup>11</onup>
        <ondown>11</ondown>
        <align>center</align>
        <focusedcolor>white</focusedcolor>
        <texturefocus colordiffuse="red" border="9">img/SettingsButtonFO.png</texturefocus>

      </control>
      <control type="button" id="11" description="Yes">
        <top>624</top>
        <centerleft>50%</centerleft>
        <width>360</width>
        <label>107</label>
        <onup>10</onup>
        <ondown>10</ondown>
        <align>center</align>
        <focusedcolor>white</focusedcolor>
        <texturefocus colordiffuse="green" border="9">img/SettingsButtonFO.png</texturefocus>

      </control>
    </control>
  </controls>
</window>
