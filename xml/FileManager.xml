<?xml version="1.0" encoding="UTF-8"?>
<window>
  <defaultcontrol always="true">20</defaultcontrol>
  <controls>
    <include>FullBackground</include>
    <control type="group">
      <include>OpenClose</include>
      <control type="label" description="Heading">
        <include>CenterHeading</include>
        <label>7</label>
      </control>
      <control type="label" id="101" description="Header Label left">
        <top>96</top>
        <include condition="[StringCompare(Skin.AspectRatio,16:9) | StringCompare(Skin.AspectRatio,4:3)]">LeftMargin</include>
        <include condition="![StringCompare(Skin.AspectRatio,16:9) | StringCompare(Skin.AspectRatio,4:3)]" content="LeftMargin">
          <param name="Left" value="120"/>
        </include>
        <right>51%</right>
        <height>60</height>
        <align>left</align>
        <label>-</label>
        <textcolor>$VAR[Label2Color]</textcolor>
        <font>size19CU</font>
        <haspath>true</haspath>
      </control>
      <control type="label" id="12" description="Footer Label Left">
        <include condition="[StringCompare(Skin.AspectRatio,16:9) | StringCompare(Skin.AspectRatio,4:3)]">LeftMargin</include>
        <include condition="![StringCompare(Skin.AspectRatio,16:9) | StringCompare(Skin.AspectRatio,4:3)]" content="LeftMargin">
          <param name="Left" value="120"/>
        </include>
        <bottom>18</bottom>
        <right>51%</right>
        <height>60</height>
        <align>left</align>
        <font>size19CU</font>
        <label>-</label>
      </control>
      <control type="panel" id="20" description="Left File List">
        <include condition="[StringCompare(Skin.AspectRatio,16:9) | StringCompare(Skin.AspectRatio,4:3)]">LeftMargin</include>
        <include condition="![StringCompare(Skin.AspectRatio,16:9) | StringCompare(Skin.AspectRatio,4:3)]" content="LeftMargin">
          <param name="Left" value="120"/>
        </include>
        <top>150</top>
        <bottom>96</bottom>
        <right>51%</right>
        <onleft>21</onleft>
        <onright>21</onright>
        <itemlayout height="80">
          <include>52listBG</include>
          <control type="label">
            <left>20</left>
            <right>140</right>
            <height>68</height>
            <font>size22C</font>
            <info>ListItem.Label</info>
          </control>
          <control type="label">
            <right>20</right>
            <height>60</height>
            <width>120</width>
            <font>size22C</font>
            <info>ListItem.Label2</info>
            <align>right</align>
          </control>
        </itemlayout>
        <focusedlayout height="80">
          <include>52listBG</include>
          <control type="label">
            <left>20</left>
            <right>140</right>
            <height>68</height>
            <font>size22C</font>
            <info>ListItem.Label</info>
          </control>
          <control type="label">
            <right>20</right>
            <height>60</height>
            <width>120</width>
            <font>size22C</font>
            <info>ListItem.Label2</info>
            <align>right</align>
          </control>
        </focusedlayout>
      </control>
      <control type="label" id="102" description="Right Header Label">
        <top>96</top>
        <include condition="[StringCompare(Skin.AspectRatio,16:9) | StringCompare(Skin.AspectRatio,4:3)]">RightMargin</include>
        <include condition="![StringCompare(Skin.AspectRatio,16:9) | StringCompare(Skin.AspectRatio,4:3)]" content="RightMargin">
          <param name="Right" value="120"/>
        </include>
        <left>51%</left>
        <height>60</height>
        <align>right</align>
        <label>-</label>
        <textcolor>$VAR[Label2Color]</textcolor>
        <font>size19CU</font>
        <haspath>true</haspath>
      </control>
      <control type="label" id="13" description="Footer Label Right">
        <include condition="[StringCompare(Skin.AspectRatio,16:9) | StringCompare(Skin.AspectRatio,4:3)]">RightMargin</include>
        <include condition="![StringCompare(Skin.AspectRatio,16:9) | StringCompare(Skin.AspectRatio,4:3)]" content="RightMargin">
          <param name="Right" value="120"/>
        </include>
        <left>51%</left>
        <height>60</height>
        <bottom>18</bottom>
        <align>right</align>
        <font>size19CU</font>
        <label>-</label>
      </control>
      <control type="panel" id="21" description="Left File List">
        <include condition="[StringCompare(Skin.AspectRatio,16:9) | StringCompare(Skin.AspectRatio,4:3)]">RightMargin</include>
        <include condition="![StringCompare(Skin.AspectRatio,16:9) | StringCompare(Skin.AspectRatio,4:3)]" content="RightMargin">
          <param name="Right" value="120"/>
        </include>
        <left>51%</left>
        <top>150</top>
        <bottom>96</bottom>
        <onleft>20</onleft>
        <onright>20</onright>
        <itemlayout height="80">
          <include>52listBG</include>
          <control type="label">
            <left>20</left>
            <right>140</right>
            <height>68</height>
            <font>size22C</font>
            <info>ListItem.Label</info>
          </control>
          <control type="label">
            <right>20</right>
            <height>60</height>
            <width>120</width>
            <font>size22C</font>
            <info>ListItem.Label2</info>
            <align>right</align>
          </control>
        </itemlayout>
        <focusedlayout height="80">
          <include>52listBG</include>
          <control type="label">
            <left>20</left>
            <right>140</right>
            <height>68</height>
            <font>size22C</font>
            <info>ListItem.Label</info>
          </control>
          <control type="label">
            <right>20</right>
            <height>60</height>
            <width>120</width>
            <font>size22C</font>
            <info>ListItem.Label2</info>
            <align>right</align>
          </control>
        </focusedlayout>
      </control>
    </control>
  </controls>
</window>
