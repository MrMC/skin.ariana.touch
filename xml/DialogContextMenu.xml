<?xml version="1.0" encoding="UTF-8"?>
<window>
  <defaultcontrol always="true">996</defaultcontrol>
  <controls>
    <include>PlainBG</include>
    <include>BackButton</include>
    <control type="label">
      <visible>!IsEmpty(ListItem.Label)</visible>
      <include>CenterHeading</include>
      <label>[B]$INFO[ListItem.Label][/B]</label>
    </control>
    <control type="grouplist" id="996" description="Context menu container">
      <include>OpenClose</include>
      <centerleft>50%</centerleft>
      <centertop>50%</centertop>
      <top>120</top>
      <bottom>120</bottom>
      <width>600</width>
      <itemgap>10</itemgap>
      <aligny>center</aligny>
      <control type="button" id="1000" description="Buttons">
        <width>600</width>
        <textoffsetx>0</textoffsetx>
        <align>center</align>
        <aligny>center</aligny>
        <height>80</height>
        <font>size22U</font>
      </control>
    </control>
  </controls>
</window>
